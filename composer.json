{
  "name": "fooman/printorderpdf-m2",
  "description": "Print the order as a pdf from the admin",
  "type": "magento2-module",
  "license": "OSL-3.0",
  "version": "2.0.1",
  "autoload": {
    "psr-4": {
      "Fooman\\PrintOrderPdf\\": "src/"
    },
    "files": [
      "src/registration.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fooman\\PrintOrderPdf\\Test\\": "tests/unit/Test/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.magento.com/"
    }
  ],
  "require-dev": {
    "magento/module-backend": "~100.0.2",
    "magento/module-sales": "~100.0.2",
    "phpunit/phpunit": "^4.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "magento-root-dir": "./vendor/magento/community-edition"
  }
}
