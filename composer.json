{
  "name": "fooman/printorderpdf",
  "description": "Print the order as a pdf from the admin",
  "require": {
    "magento/magento-composer-installer": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "magento/community-edition": "dev-master"
  },
  "type": "magento2-module",
  "license": "OSL-3.0",
  "version": "1.0.5",
  "autoload": {
    "psr-0": {
      "Magento\\": "vendor/magento/community-edition/app/code/"
    },
    "psr-4": {
      "Fooman\\": "app/code/Fooman/"
    }
  },
  "extra": {
    "map": [
      [
        "app/code/Fooman/PrintOrderPdf/*",
        "Fooman/PrintOrderPdf"
      ]
    ]
  }
}
